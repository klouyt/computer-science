function b1() {
  if (document.getElementById('b1').style.backgroundColor=="white"){
    document.getElementById('b1').style.backgroundColor = "black";
  } 
  else {
    document.getElementById('b1').style.backgroundColor = "white";
  }
  if (document.getElementById('b2').style.backgroundColor=="white"){
    document.getElementById('b2').style.backgroundColor = "black";
